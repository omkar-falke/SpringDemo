<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
			"http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>

	<bean id="c" class="com.demo.Deparment">
		<property name="fruit">
			<list>
				<value>Apple</value>
				<value>Mango</value>
			</list>
		</property>

		<property name="cricketer">
			<set>
				<value>sachin</value>
				<value>Rohit</value>
			</set>
		</property>

		<property name="cc">
			<map>
				<entry key="india" value="dealhi"></entry>
				<entry key="USA" value="Washington"></entry>
			</map>
		</property>
	</bean>
</beans>